package com.netmarch.figurecloud.auth.core.security.exceptions;

import org.springframework.security.core.AuthenticationException;

public class GeneralAuthenticationException  extends AuthenticationException {
    public GeneralAuthenticationException(String msg) {
        super(msg);
    }
    public GeneralAuthenticationException(String msg,Throwable throwable) {
        super(msg,throwable);
    }
}
