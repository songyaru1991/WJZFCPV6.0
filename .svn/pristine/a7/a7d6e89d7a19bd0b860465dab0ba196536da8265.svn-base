public class AreaCodeUtil {
    public static String handle(String areaCode) {
        if(areaCode.endsWith("000")){
            return areaCode.substring(0,3);
        }else if(areaCode.endsWith("00")){
            return areaCode.substring(0,4);
        }else{
            return areaCode;
        }
    }
}
